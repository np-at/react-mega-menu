{
  "name": "react-mega-menu",
  "version": "1.0.0",
  "description": "react-mega-menu React component",
  "main": "lib/index.js",
  "types": "lib/main.d.ts",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "moveDeclaration": "cp src/index.d.ts lib/index.d.ts",
    "tsc": "tsc --declaration",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run tsc && npm run build && npm run moveDeclaration",
    "start": "nwb serve-react-demo",
    "typecheck": "tsc --noEmit",
    "tslint": "tslint --project tsconfig.json",
    "test": "npm run typecheck && npm run tslint && npm run jest",
    "jest": "jest",
    "jest:coverage": "jest --coverage"
  },
  "dependencies": {
    "glamor": "^2.20.40"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^23.6.0",
    "nwb": "^0.25.2",
    "nwb-sass": "^0.10.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "author": {
    "email": "b.hen@supply.com",
    "name": "Baruch-Adi Hen"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "collectCoverageFrom": [
      "src/index.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "homepage": "",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/SUPPLYcom/react-mega-menu",
    "type": "git"
  },
  "keywords": [
    "react-component",
    "navigation",
    "menu",
    "aim-menu",
    "mega-menu"
  ]
}
